#include <stdio.h>
float lamtron(float a) //9.17->9.2
{
  float i;
  i=a*10-(int)(a*10);
  if (i<0.5) a=(int)(a*10) 10;x
  else a=((int)(a*10)+1)/10;
  return a;
}
int main()
{
  float a=9.14,b=2.5;
  printf("%d %d\n",(int)(a*10),(int)(b*10));
  a=lamtron(a);
  b=lamtron(b);
  printf("%f %f\n",a,b);
}
